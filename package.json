{
  "name": "real-time-launchpad",
  "version": "0.2.0",
  "private": true,
  "author": "Rich Lewis",
  "description": "A modern e-commerce marketplace application showcasing React 19's latest features",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
